{
  "name": "yeap-front-snippets",
  "displayName": "Yeap Front Snippets",
  "description": "Frontend snippets for Yeap development",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript", 
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "yeap-front-snippets.refreshSnippets",
        "title": "Yeap Snippets: Refresh from Server"
      },
      {
        "command": "yeap-front-snippets.insertSnippet",
        "title": "Yeap Snippets: Show Info"
      },
      {
        "command": "yeap-front-snippets.clearCache",
        "title": "Yeap Snippets: Clear Cache"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}